*                   text=auto eol=lf

# override for Windows/Visual Studio
*.bat           eol=crlf
*.sln           eol=crlf
*.csproj        eol=crlf
misc/msvs/*     eol=crlf
# Godot scene & script formats
*.tscn          text eol=lf
*.gd            text eol=lf
*.gdc           text eol=lf
*.shader        text eol=lf
*.tres          text eol=lf
*.gdns          text eol=lf
*.gdnlib        text eol=lf
*.godot         text eol=lf
# Audio
*.wav           binary
*.mp3           binary
*.ogg           binary
*.flac          binary
*.opus          binary
# Video
*.mp4           binary
*.mkv           binary
*.avi           binary
*.mov           binary
# Images & HDRs
