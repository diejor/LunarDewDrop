*                       text=auto eol=lf

# Windows/Visual Studio overrides
*.bat                   eol=crlf
*.sln                   eol=crlf
*.csproj                eol=crlf
misc/msvs/*             eol=crlf
*.tscn                  text eol=lf
*.gd                    text eol=lf
*.gdc                   text eol=lf
*.shader                text eol=lf
*.tres                  text eol=lf
*.gdns                  text eol=lf
*.gdnlib                text eol=lf
*.godot                 text eol=lf
*.import                text eol=lf
assets/**               filter=lfs diff=lfs merge=lfs -text
